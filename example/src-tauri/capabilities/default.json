{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "shell:allow-open",
    "nspopover:allow-show-popover",
    "nspopover:allow-hide-popover",
    "nspopover:allow-is-popover-shown",
    "core:tray:allow-new",
    "core:tray:default"
  ]
}
